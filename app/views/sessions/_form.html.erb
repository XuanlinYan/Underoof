<div class="row">
    <br><br><br>
</div>
<div class="row">
    <div class="col-3"></div>
    <div class="col-6">
        <%= form_with(url: signin_path, scope: :session, local: true) do |f| %>
            <div class="form-group form-icons">
                <span class="fa fa-envelope errspan"></span>
                <%= f.email_field :email, class: "form-control", placeholder: "Email" %>
            </div>

            <div class="form-group form-icons">
                <span class="fa fa-unlock-alt errspan"></span>
                <%= f.password_field :password, class: "form-control", placeholder: "Password" %>
            </div>
            <%= f.submit "Sign in", class: "btn btn-dark col" %>
        <% end %>
    </div>
    <div class="col-3"></div>
</div>
<div class="row">
    <br><br><br>
    <br><br><br>
    <br>
</div>