
<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Gender</th>

    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>

        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.gender %></td>

      </tr>
    <% end %>
  </tbody>
</table>
